pipeline {
    agent any
    stages {
            stage('Welcome') {
                steps {
                 echo 'Hi! you are inside the pipeline'
                 }
            }
            stage('Build') {
                steps {
                echo 'skipping build'
                }
            }
            stage('Test') {
                steps {
                echo 'skipping test'
                }
            }
    }
}